programa
{
    funcao inicio()
    {
        inteiro num1, num2, resto

        escreva("Digite o primeiro número inteiro: ")
        leia(num1)

        escreva("Digite o segundo número inteiro: ")
        leia(num2)

        resto = num1 % num2

        escreva("O resto da divisão de ", num1, " por ", num2, " é: ", resto, "\n")
    }
}
